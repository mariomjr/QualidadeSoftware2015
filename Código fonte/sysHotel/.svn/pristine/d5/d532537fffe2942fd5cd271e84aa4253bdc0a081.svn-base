package org.sysHotel.enums;

public enum SessionUser {

	SESSION_USER_REGISTERED("session_user_registered", null),
	PAGE_INDEX("/home.xhtml", "index"),
	PAGE_LOGIN("/login.xhtml", "login");

	private String context;

	private String name;

	private SessionUser(String context, String name) {
		this.context = context;
		this.name = name;
	}

	public String getContext() {
		return context;
	}

	public void setContext(String context) {
		this.context = context;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
